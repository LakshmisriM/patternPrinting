# patternPrinting
pattern_printing
import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    int i,j;
		System.out.println("0");
	    for(i=9;i>=n;i--)
		{
		    for(j=i;j<=9;j++)
		    {
		        System.out.print(j);
		    }
		     System.out.print("0");
		    for(j=9;j>=i;j--)
		    {
		        System.out.print(j);
		    }
		    System.out.println("");
		}
		
	}
}
